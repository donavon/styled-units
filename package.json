{
  "name": "styled-units",
  "version": "1.1.1",
  "description": "A unit helper for styled-components.",
  "main": "./lib/index.js",
  "scripts": {
    "prepublish": "npm run lint && npm run _build && npm run _test",
    "build": "npm run lint && npm run _build",
    "_build": "babel src --out-dir lib",
    "test": "npm run lint && npm run _test",
    "_test": "jest",
    "lint": "eslint src"
